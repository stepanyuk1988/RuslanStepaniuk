print('Enter operator')
c = str(input())
print('Enter second number')
b = int(input())

if c == '+':
    print(a + b)
elif c == '-':
    print(a - b)
elif c == '*':
    print(a * b)
elif c == '/':
    print(a / b)
elif c == '%':
    print(a % b)


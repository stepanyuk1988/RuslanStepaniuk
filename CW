def decorator(func):
    def wrapper():
        print("Madina")
        func()
        print("5.23.2020")

    return wrapper


def decorator2(func2):
    def wrapper2():
        print("12.13.1992")
        func2()
        print("May")

    return wrapper2


@decorator
@decorator2
def print_text():
    print("This is one more line")


print_text()

